      "name": "GenBank",
      "url": "https://www.ncbi.nlm.nih.gov/genbank/"
    },
    {
      "name": "Pathoplexus",
      "url": "https://pathoplexus.org"
    }
  ],
  "build_url": "https://github.com/nextstrain/hmpv",
  "colorings": [
    {
      "key": "gt",
      "title": "Genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "Date",
      "type": "continuous"
    },
    {
      "key": "country",
      "title": "Country",
      "type": "categorical"
    },
    {
      "key": "dataUseTerms",
      "title": "Data use terms",
      "type": "categorical"
    },
    {
      "key": "clades",
      "title": "Clades",
      "type": "categorical"
    },
    {
      "key": "region",
      "title": "Region",
      "type": "categorical"
    },
    {
      "key": "insertion",
      "title": ">30bp insertion in G gene",
      "type": "boolean"
    },
    {
      "key": "old_nomenclature",
      "title": "Old Clade Names",
      "type": "categorical"
    }
  ],
  "metadata_columns": [
    "PPX_accession",
    "INSDC_accession",
    "restrictedUntil"
