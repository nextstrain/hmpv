{
  "title": "Human metapneumovirus phylogeny",
  "maintainers": [
    {"name": "Richard Neher", "url": "https://neherlab.org/richard-neher.html"},
    {"name": "Isabel Joia", "url": "https://neherlab.org"},
    {"name": "the Nextstrain team", "url": "https://nextstrain.org/team"}
  ],
  "data_provenance": [
